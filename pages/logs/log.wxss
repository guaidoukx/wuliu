/* pages/logs/log.wxss */
page{
  width: 100%;
  height: 100%;
}
.main{
  width: 100%;
  height: 100%;
  background: #ffffff;
  position: relative;
}
.box{
  width: 604rpx;
  height: 252rpx;
  position: absolute;
  top:0;
  left:0;
  bottom: 0;
  right: 0;
  margin:auto;
}
.log{
  width: 604rpx;
  height: 82rpx;
  margin: auto;
  position: relative
}
.wechat{
  width: 42rpx;
  height: 30rpx;
  position: absolute;
  top:0;
  bottom: 0;
  margin: auto;
  left: 138rpx;
  z-index: 99
}
.logBtn{
  width: 600rpx;
  background: #43ae3d;
  height: 78rpx;
  border-radius: 39rpx;
  color: #ffffff;
  font-size: 30rpx;
  line-height: 78rpx;
}
.logBtn::after{
  border:2px solid #009000;
}
.or{
  display: flex;
  justify-content: space-between;
  width: 172rpx;
  height: 22rpx;
  margin:auto;
  font-size: 22rpx;
  line-height: 22rpx;
  margin-top: 60rpx;
}
.line{
  width: 48rpx;
  height: 2rpx;
  background: #b6b6b6;
  margin-top: 10rpx;
}
.huozhe{
  color: #b4b4b4
}
.more{
  width: 100%;
  height: 24rpx;
  text-align: center;
  line-height: 24rpx;
  font-size: 24rpx;
  color: #1b1b1b;
  margin-top: 66rpx;
}
.elastic{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background-color: rgba(0,0,0,0.5);
  z-index: 100;
}
.elastic .elaflm{
  width: 100%;
  height:627rpx;
  background-color: #FFFFFF;
  position: absolute;
  left: 0;
  bottom: 0;
  padding: 54rpx 50rpx 0 52rpx;
  box-sizing: border-box;
}
.elaflm .head{
  width: 100%;
  height: 46rpx;
  line-height: 46rpx;
}
.elaflm .head .logo{
  float: left;
}
.elaflm .head view{
  float: left;
  height: 46rpx;
  font-size: 29rpx;
}
.elaflm .head .hte1{
  font-weight: bold;
  margin-left: 34rpx;
  color: #020202;
}
.elaflm .head .hte2{
  margin-left: 58rpx;
  color: #050505;
}
.elaflm .head .tips{
  float: right;
  margin-top: 8rpx;
}
.elaflm .phonetext{
  font-size: 44rpx;
  font-weight: bold;
  margin-top: 37rpx;
  margin-bottom: 44rpx;
}
.elaflm .phone{
  height: 108rpx;
  padding: 0 5rpx;
  border-top:2px solid #F4F4F4;
  border-bottom:2px solid #F4F4F4;
}
.elaflm .phone view{
  float: left;
  line-height: 108rpx;
  font-size: 24rpx;
  
}
.elaflm .phone .text{
  margin-left: 26rpx;
  color: #A9A9A9;
}
.elaflm .phone .dui{
  float: right;
  margin-top: 35rpx;
}
.elaflm .other{
  color: #5C5C7A;
  font-size: 26rpx;
  margin-top:41rpx;
} 
.btn{
  height: 78rpx;
  width: 100%;
  margin-top: 104rpx;
}
.btn button{
  float: left;
  width: 311rpx;
  height: 78rpx;
  border:0;
  outline: none;
  line-height: 78rpx;
  font-size: 32rpx;
}
.btn .jujue{
  background-color: #ECECEC;
  color: #040404;
}
.btn .yunxu{
  margin-left: 28rpx;
  background-color: #1DAA1A;
  color: #fff;
}
.show{
  display: block
}
.hidden{
  display: none
}