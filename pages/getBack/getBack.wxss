/* pages/getBack/getBack.wxss */
page{
  background: #ededed;
}
.checkTime{
  width: 94%;
  height: 192rpx;
  margin: 16rpx auto 0;
  background: #ffffff;
  border-radius: 8rpx;
  overflow: hidden
}
.date{
  width: 92%;
  height: 72rpx;
  margin:auto;
}
.pickers{
  width: 100%;
  height: 72rpx;
  margin:auto;
  display: flex;
  justify-content: space-between
}
.year,.month,.day{
  width: 178rpx;
  height: 68rpx;
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  color: #999999;
  line-height: 72rpx;
}
.datas{
  width: 136rpx;
  height: 68rpx;
  border: 2rpx solid #e6e6e6;
  background: #f6f6f6;
  border-radius: 6rpx;
  text-align: center;
}
.addr{
  width: 94%;
  height: 550rpx;
  margin: 16rpx auto 0;
  background: #ffffff;
  border-radius: 8rpx;
  overflow: hidden
}
.addrinfo{
  width: 92%;
  height: 88rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  line-height: 88rpx;
}
.title{
  font-size: 32rpx;
  color: #333333;
  font-weight: bold
}
.addrCheck{
  width: 236rpx;
  font-size: 22rpx;
  color:#999999;
  display: flex;
  justify-content: space-between
}
radio .wx-radio-input{
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
}
radio .wx-radio-input.wx-radio-input-checked::before{
  width: 48rpx;
  height: 48rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 50%;
  background: #3d4663;
  border: none;
}
.line{
  width: 2rpx;
  height: 26rpx;
  background: #999999;
  margin-top: 32rpx;
}
.addrPic{
  width: 28rpx;
  height: 31rpx;
  display: block;
  margin-top: 30rpx;
}
#name,#tel{
  width: 88%;
  height: 70rpx;
  margin:auto;
  border:2rpx solid #e6e6e6;
  font-size: 24rpx;
  color: #999999;
  padding: 0 0 0 24rpx;
}
#tel{
  margin-top: 16rpx;
}
.name,.tel{
  font-size: 24rpx;
  color: #999999;
}
.picker{
  font-size: 24rpx;
  color: #999999;
  line-height: 70rpx;
  width: 88%;
  height: 70rpx;
  border:2rpx solid #e6e6e6;
  margin: 16rpx auto 0; 
  display: flex;
  justify-content: space-between;
  padding: 0 0 0 24rpx;
}
.jiantou{
  width: 12rpx;
  height: 22rpx;
  margin:22rpx 22rpx 0 0;
}
textarea{
  width: 84%;
  height: 126rpx;
  font-display: block;
  border: 2rpx solid #e6e6e6;
  margin: 16rpx auto 0;
  font-size: 24rpx;
  padding: 22rpx 24rpx 0;
  color: #999999;
}
.dizhi{
  color: #999999;
}
.confirm{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  font-size: 28rpx;
  line-height: 98rpx;
  text-align: center;
  background: #3d4663;
  color: #ffffff;
  border-radius: 0;
}
.baojia{
  width: 94%;
  height: 90rpx;
  margin: 16rpx auto;
  background: #ffffff;
  border-radius: 8rpx;
  display: flex;
  justify-content: space-between;
}
.baojia_word{
  height: 32rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: bold;
  width: 128rpx;
  margin: 28rpx 28rpx;
}
.baojia_radio{
  margin:14rpx 28rpx;
}
.rexian{
  width: 100%;
  height: 88rpx;
  bottom: 98rpx;
  left: 0
}
.tels{
  width: 100%;
  height: 88rpx;
  font-size: 24rpx;
  line-height: 88rpx;
  text-align: center;
  color: #666666
}
.kefu{
  width: 200rpx;
  height: 54rpx;
  background: #aaaaaa;
  border-top-left-radius: 27rpx;
  border-bottom-left-radius: 27rpx;
  position: fixed;
  bottom:118rpx;
  right: 0;
  display: flex;
  justify-content: space-around
}
.kufu_pic{
  width: 32rpx;
  height: 34rpx;
  display: block;
  margin: 10rpx 0 0 26rpx;
}
.kefu_describe{
  width: 102rpx;
  height: 54rpx;
  font-size: 24rpx;
  line-height: 54rpx;
  color:#ededed;
}