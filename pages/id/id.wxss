/* pages/carNum/carNum.wxss */
/* pages/name/name.wxss */
@import "../../iconfont.wxss";

page {
  height: 900rpx;
}

.list_content {
  vertical-align: middle;
  align-content: center;
  width: 90%;
  height: 300rpx;
  background: #fff;
  margin: 300rpx auto;
  border-radius: 8rpx;
}

.list {
  width: 92%;
  margin: auto;
  height: 96rpx;
  border-bottom: 2rpx solid #e6e6e6;
  display: flex;
  justify-content: space-between;
}

.list_left {
  width: 160rpx;
  height: 34rpx;
  display: flex;
  justify-content: space-between;
  margin: 36rpx 0 0 10rpx;
}

.listIcon {
  width: 34rpx;
  height: 34rpx;
}

.listName {
  line-height: 34rpx;
  font-size: 30rpx;
  color: #404040;
}

.jiantou {
  width: 16rpx;
  height: 28rpx;
  margin-top: 30rpx;
}

.list_right {
  width: 50%;
  height: 34rpx;
  display: flex;
  vertical-align: middle;
  /* line-height: 1; */
  justify-content: space-between;
}

.check {
  font-size: 30rpx;
  /* line-height: 28rpx; */
  color: #999;
  margin-top: 30rpx;
}

.signOut {
  width: 84%;
  height: 78rpx;
  margin: auto;
  font-size: 30rpx;
  color: #fff;
  border-radius: 39rpx;
  background: #3e4663;
  margin-top: 50rpx;
}

.iconfont {
  color: #3e4663;
  /* font-size: 20rpx; */
  display: inline-flex;
}

.form{
  width: 100%;
  height: 780rpx;
  margin-top: 16rpx;
  background: #ffffff;
}
.inputBox{
  width: 100%;
  height: 90rpx;
  border-bottom:2rpx solid #ededed;
  display: flex;
  justify-content: space-between;
  padding:0 0 0 24rpx;
}
label{
  font-size: 32rpx;
  font-weight: bold;
  line-height: 90rpx;
}
#editItem{
  width: 70%;
  height: 90rpx;
  margin-right: 48rpx;
  text-align: right;
  font-size: 24rpx;
  color: #999999;
}
.editItem{
  font-size: 24rpx;
  line-height: 90rpx;
  color: #999999;
}
#type{
  width: 65%;
  height: 90rpx;
  margin-right: 48rpx;
  text-align: right;
  font-size: 24rpx;
  color: #999999;
}
.type{
  font-size: 24rpx;
  line-height: 90rpx;
  color: #999999;
}
.inputBoxCode{
  width: 100%;
  height: 90rpx;
  border-bottom:2rpx solid #ededed;
  display: flex;
  justify-content: space-around;
  padding:0 0 0 24rpx;
}
.submit{
  width: 100%;
  height: 98rpx;
  position: absolute;
  bottom:0;
  left:0;
  background: #3d4663;
  color: #ffffff;
  font-size: 30rpx;
  line-height: 98rpx;
  text-align: center;
  border-radius: 0;
}
