<form bindsubmit="formSubmit"> 
  <view class='form'>
    <view class='inputBox'>
      <label for='editItem'>手机号</label>
      <input id="editItem" name="editItem" auto-focus type='number' value='{{tel}}' placeholder='请输入手机号'></input>
    </view>
  </view>
  <button class='submit' form-type='submit'>保  存</button>
</form>